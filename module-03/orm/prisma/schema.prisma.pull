model user {
  id         Int     @id @default(autoincrement())
  first_name String? @db.VarChar(30)
  last_name  String? @db.VarChar(30)
  email      String? @db.VarChar(30)
  password   String? @db.VarChar(30)
}

model role {
  id   Int     @id @default(autoincrement())
  name String? @db.VarChar(30)
}
